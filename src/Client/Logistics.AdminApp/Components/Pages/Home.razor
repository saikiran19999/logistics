@page "/"

<PageTitle>Logistics Admin</PageTitle>
<RadzenText TextStyle="TextStyle.H3">Dashboard</RadzenText>

<table class="table table-hover">
    <thead>
    <tr class="row bg-light">
        <th class="col-6 col-sm-7 col-md-8 col-lg-9">Catalog</th>
        <th class="col-6 col-sm-5 col-md-4 col-lg-3">Action</th>
    </tr>
    </thead>
    <tbody>
    <tr class="row">
        <td class="col-6 col-sm-7 col-md-8 col-lg-9">
            <span class="fw-bold">Tenants</span>
        </td>
        <td class="col-6 col-sm-5 col-md-4 col-lg-3">
            <RadzenButton Icon="create"
                          Click="@(() => Navigation.NavigateTo("/tenants/add"))">
            </RadzenButton>
            <RadzenButton Icon="list"
                          Click="@(() => Navigation.NavigateTo("/tenants"))">
            </RadzenButton>
        </td>
    </tr>
    </tbody>
</table>
